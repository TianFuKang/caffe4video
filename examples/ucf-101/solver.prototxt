net: "examples/ucf-101/ucf101-train-test.prototxt"
test_iter: 100
test_interval: 100
base_lr: 0.01
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 20
display_norm: 100
max_iter: 45000
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000 
snapshot_prefix: "examples/ucf-101/model/ucf101-train"
solver_mode: GPU
device_id: 1

